{
  "expo": {
    "name": "Baqylau App",
    "slug": "baqylau",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs to access your location to show you your current position.",
        "NSLocationAlwaysUsageDescription": "This app needs to access your location to show you your current position."
      },
      "bundleIdentifier": "kz.baqylau.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA52y_n7gneWfP2Q7zHB1mxR-qGJSa3mQE"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "kz.baqylau.app",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA52y_n7gneWfP2Q7zHB1mxR-qGJSa3mQE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fa0f83cf-ea9f-4f15-be21-56813ff4937a"
      }
    }
  }
}
